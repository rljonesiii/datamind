# LLM API Keys
OPENAI_API_KEY=your_openai_api_key_here
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# Optional: Disable real API for testing (default is true)
# DSASSIST_USE_REAL_API=false

# Optional: Custom LLM endpoints
CUSTOM_LLM_ENDPOINT=http://localhost:8000

# Execution environment
JULIA_NUM_THREADS=4
PYTHON_ENV_PATH=/path/to/python/env

# Knowledge graph (if using external graph database)
NEO4J_URI=bolt://localhost:7687
NEO4J_USER=neo4j
NEO4J_PASSWORD=password

# Redis for message bus (if using distributed setup)
REDIS_URL=redis://localhost:6379

# Monitoring and logging
LOG_LEVEL=INFO
EXPERIMENT_DATA_DIR=./experiments